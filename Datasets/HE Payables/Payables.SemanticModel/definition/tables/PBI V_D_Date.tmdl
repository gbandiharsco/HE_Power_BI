	column 'Current month Prompt' =
			IF(
			    MONTH('PBI V_D_Date'[FullDate]) = MONTH(TODAY()) && YEAR('PBI V_D_Date'[FullDate]) = YEAR(TODAY()),
			    "Current Month",
			    FORMAT('PBI V_D_Date'[FullDate], "MMM-YY"))
		lineageTag: b5e75da9-2f83-4553-a606-c59af8049129
		summarizeBy: none
		sortByColumn: YearMonth

		changedProperty = SortByColumn

		annotation SummarizationSetBy = Automatic

	column 'Flagged Year' =
			
			IF(YEAR('PBI V_D_Date'[FullDate]) >= YEAR(TODAY()) - 3 && YEAR('PBI V_D_Date'[FullDate]) <= YEAR(TODAY()) &&
			  ((YEAR('PBI V_D_Date'[FullDate]) = YEAR(TODAY()) - 3 && MONTH('PBI V_D_Date'[FullDate]) >= MONTH(TODAY())) ||
			  (YEAR('PBI V_D_Date'[FullDate]) = YEAR(TODAY()) && MONTH('PBI V_D_Date'[FullDate]) <= MONTH(TODAY()) + 6)),"Y","N")
		lineageTag: e23f3b0c-42f2-43ed-920d-4b0ed51bde51
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

